{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    no-multi-spaces: ["error", { exceptions: { "ImportDeclaration": true } }],
    react/jsx-curly-spacing: [2, {"when": "always", "allowMultiline": false}],
    prettier/prettier: "error"
  },
  "env": {
    "browser": true
  }
}
